                if( parentURL.indexOf("/admin/themes/{{ theme.id }}/editor") != -1 ){ 
                    window.location.assign('/index?view=stealth');
                 }else{
                    var url = document.URL.replace("{{ pageUrl | split: '.com' | first }}.com", '');
                    if(url.length > 1 && url.indexOf('/pages/error') == -1){
                      if (typeof(Storage) !== "undefined") {
                          sessionStorage.pepsiPrintURL = url;
                      }
                      else
                        parent.location.assign('/account/login?redirect=' + url);

                    }
                    parent.location.assign('/account/login');
                 }
